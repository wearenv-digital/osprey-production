<div id="cctv" class="categories-holder marine-cameras cctv" data-ix="fade-on-scroll-2">
	<% data.forEach(function(el){ %>
		<div class="individual-category marine-camera-product main-categories" id="main-categoriesId">
			<a href="<%= `/product-page/${el.product_code}` %>">
				<div class="marine">
					<img class="product-list-image" src="<%=`${el.image_link}` %>" alt="">
				</div>
				<br>
				<%= el.product_name %>
			</a>
		</div>
		<% }) %>
</div>